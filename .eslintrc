{
  "extends": "airbnb-base",
  "globals": {
    "Logger": true,
    "Models": true,
    "Server": true
  },
  "rules": {
    "comma-dangle": 0,
    "global-require": 0,
    "no-unused-vars": 0,
    "no-underscore-dangle": 0,
    "import/no-unresolved": 0,
  },
  "env": {
    "node": true,
    "mocha": true
  }
}
